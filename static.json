{
  "root": "build/",
  "routes": {
    "/images/*": "/images/",
    "/static/media/*": "/static/media/",
    "/static/css/*": "/static/css/",
    "/static/js/*": "/static/js/",
    "/static/java/*": "/static/java/",
    "/**": "index.html"
  },
  "clean_urls": true,
  "https_only": true,
  "headers": {
    "/**": {
      "Strict-Transport-Security": "max-age=31557600"
    }
  }
}
